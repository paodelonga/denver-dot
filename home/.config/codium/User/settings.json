{
  "apc.electron": {
    "frame": false
  },
  "apc.header": {
    "height": 32
  },
  "apc.listRow": {
    "height": 24
  },
  "apc.stylesheet": {
    ".monaco-workbench .monaco-list:not(.element-focused):focus:before": "display: flex;",
    ".split-view-view visible": "height: 0;"
  },
  "debug.toolBarLocation": "docked",
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.renderGutterMenu": false,
  "diffEditor.renderSideBySide": false,
  "editor.accessibilitySupport": "off",
  "editor.autoClosingBrackets": "never",
  "editor.copyWithSyntaxHighlighting": false,
  "editor.dragAndDrop": false,
  "editor.folding": true,
  "editor.fontFamily": "Maple Mono NF",
  "editor.fontLigatures": true,
  "editor.fontVariations": true,
  "editor.fontWeight": "bold",
  "editor.mouseWheelScrollSensitivity": 2,
  "editor.parameterHints.cycle": false,
  "editor.quickSuggestions": {
    "comments": "on",
    "other": "on",
    "strings": "on"
  },
  "editor.renderLineHighlight": "none",
  "editor.renderWhitespace": "boundary",
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "visible",
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "editor.tabSize": 4,
  "editorconfig.generateAuto": false,
  "editorconfig.template": "/home/paodelonga/.editorconfig",
  "explorer.autoRevealExclude": {
    "**/.git": true
  },
  "explorer.compactFolders": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.fileNesting.enabled": true,
  "explorer.sortOrder": "foldersNestsFiles",
  "extensions.autoUpdate": false,
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "*.ino": "cpp"
  },
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/CVS": true,
    "**/Thumbs.db": true,
    ".DS_Store": true,
    ".venv": true,
    "__pycache__": true
  },
  "git.alwaysSignOff": true,
  "git.branchPrefix": "`<group>.<subject>.<uuid4|commit_tag>.[username]`",
  "git.defaultCloneDirectory": "/home/paodelonga/repositories",
  "git.enableCommitSigning": true,
  "security.workspace.trust.banner": "never",
  "security.workspace.trust.startupPrompt": "never",
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.allowMnemonics": true,
  "terminal.integrated.customGlyphs": false,
  "terminal.integrated.defaultLocation": "view",
  "terminal.integrated.defaultProfile.linux": "fish",
  "terminal.integrated.fontFamily": "monospace",
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.gpuAcceleration": "off",
  "update.mode": "none",
  "update.showReleaseNotes": false,
  "window.customMenuBarAltFocus": false,
  "window.customTitleBarVisibility": "auto",
  "window.openFoldersInNewWindow": "on",
  "window.openWithoutArgumentsInNewWindow": "on",
  "window.titleBarStyle": "custom",
  "window.titleSeparator": ", ",
  "workbench.colorCustomizations": {},
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.labelFormat": "short",
  "workbench.enableExperiments": false,
  "workbench.layoutControl.enabled": false,
  "workbench.startupEditor": "none",
  "workbench.tree.renderIndentGuides": "always",
  "arduino.useArduinoCli": true,
  "arduino.clearOutputOnBuild": true,
  "arduino.enableUSBDetection": true,
  "arduino.logLevel": "verbose",
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "C_Cpp.codeAnalysis.clangTidy.enabled": true,
  "editor.rulers": [120],
  "git.confirmSync": false,
  "gitlens.views.branches.branches.layout": "list",
  "css.autoValidation": "Always",
  "css.enabledLanguages": ["html"],
  "css.styleSheets": ["assets/styles/**/*.{css,scss}"],
  "cssPeek.trace.server": "messages",
  "files.trimFinalNewlines": false,
  "liveServer.settings.wait": 50,
  "liveServer.settings.useLocalIp": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "html.trace.server": "verbose",
  "editor.formatOnSave": false,
  "eslint.format.enable": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "always"
  },
  "gitlens.launchpad.indicator.label": "counts",
  "editor.fontSize": 16,
  "editor.cursorStyle": "block-outline",
  "workbench.editor.enablePreview": false,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.foldingHighlight": false,
  "editor.hideCursorInOverviewRuler": true,
  "editor.lineHeight": 1.2,
  "editor.links": false,
  "editor.scrollbar.verticalScrollbarSize": 8,
  "editor.stickyScroll.enabled": false,
  "workbench.activityBar.location": "top",
  "breadcrumbs.enabled": false,
  "workbench.editor.editorActionsLocation": "hidden",
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.tabActionCloseVisibility": false,
  "workbench.editor.untitled.labelFormat": "name",
  "window.commandCenter": false,
  "explorer.decorations.badges": false,
  "window.autoDetectColorScheme": true,
  "workbench.preferredLightColorTheme": "Solarized Light",
  "window.menuBarVisibility": "compact"
}
