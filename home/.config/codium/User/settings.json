{
  "editor.fontSize": 13,
  "editorconfig.template": "/root/.editorconfig",
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.enabled": false,
  "editor.dragAndDrop": false,
  "editor.mouseWheelScrollSensitivity": 2,
  "window.titleBarStyle": "custom",
  "window.customMenuBarAltFocus": false,
  "editor.minimap.enabled": false,
  "editor.glyphMargin": false,
  "editor.folding": false,
  "notebook.lineNumbers": "on",
  "editor.fontFamily": "'Maple Mono NF', 'monospace', monospace",
  "hediet.vscode-drawio.resizeImages": null,
  "explorer.confirmDelete": false,
  "merge-conflict.codeLens.enabled": false,
  "merge-conflict.decorators.enabled": false,
  "hediet.vscode-drawio.defaultEdgeStyle": {},
  "security.workspace.trust.banner": "never",
  "security.workspace.trust.startupPrompt": "never",
  "security.workspace.trust.untrustedFiles": "open",
  "update.mode": "none",
  "update.showReleaseNotes": false,
  "accessibility.accessibleView.closeOnKeyPress": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.decorations.colors": false,
  "git.alwaysSignOff": true,
  "git.branchPrefix": "`<group>.<subject>.<uuid4|commit_tag>.[username]`",
  "git.branchRandomName.dictionary": [
    "adjectives",
    "animals",
    "colors"
  ],
  "git.defaultCloneDirectory": "/root/repositories",
  "git.enableCommitSigning": true,
  "window.customTitleBarVisibility": "auto",
  "workbench.activityBar.location": "hidden",
  "terminal.integrated.allowMnemonics": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.defaultLocation": "editor",
  "terminal.integrated.defaultProfile.linux": "fish",
  "terminal.integrated.fontSize": 12,
  "terminal.integrated.localEchoExcludePrograms": [
    "vim",
    "vi",
    "nano",
    "tmux",
    "micro",
    "lvim",
    "lunarvim",
    "ed",
    "editor"
  ],
  "terminal.integrated.rightClickBehavior": "nothing",
  "window.menuBarVisibility": "compact",
  "workbench.startupEditor": "none",
  "glassit.alpha": 125,
  "workbench.colorTheme": "Solarized Light",
}
