  "editorconfig.template": "/home/paodelonga/.editorconfig",
  "explorer.autoRevealExclude": {
    "**/.git": true
  },
  "explorer.compactFolders": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.decorations.badges": false,
  "explorer.fileNesting.enabled": true,
  "explorer.sortOrder": "foldersNestsFiles",
  "extensions.autoUpdate": false,
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "*.ino": "cpp"
  },
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/CVS": true,
    "**/Thumbs.db": true,
    ".DS_Store": true,
    ".venv": true,
    "__pycache__": true
  },
  "git.alwaysSignOff": true,
  "git.branchPrefix": "`<group>.<subject>.<uuid4|commit_tag>.[username]`",
  "git.defaultCloneDirectory": "/home/paodelonga/repositories",
  "git.enableCommitSigning": true,
  "security.workspace.trust.banner": "never",
  "security.workspace.trust.startupPrompt": "never",
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.allowMnemonics": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.customGlyphs": false,
  "terminal.integrated.defaultLocation": "view",
  "terminal.integrated.defaultProfile.linux": "fish",
  "terminal.integrated.fontFamily": "monospace",
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.gpuAcceleration": "off",
  "update.mode": "none",
  "update.showReleaseNotes": false,
  "window.commandCenter": false,
  "window.confirmBeforeClose": "keyboardOnly",
  "window.customMenuBarAltFocus": false,
  "window.customTitleBarVisibility": "auto",
  "window.openFoldersInNewWindow": "on",
  "window.openWithoutArgumentsInNewWindow": "on",
  "window.titleBarStyle": "custom",
  "window.titleSeparator": ", ",
  "workbench.activityBar.location": "hidden",
  "workbench.colorCustomizations": {},
  "workbench.colorTheme": "Default Dark+",
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.enablePreview": false,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.labelFormat": "short",
  "workbench.editor.tabActionCloseVisibility": false,
  "workbench.enableExperiments": false,
  "workbench.layoutControl.enabled": false,
  "workbench.startupEditor": "none",
  "workbench.tips.enabled": false,
  "workbench.tree.indent": 16,
  "workbench.tree.renderIndentGuides": "none",
  "arduino.useArduinoCli": true,
  "arduino.clearOutputOnBuild": true,
  "arduino.enableUSBDetection": true,
  "arduino.logLevel": "verbose",
  "workbench.editor.editorActionsLocation": "hidden",
  "window.menuBarVisibility": "compact",
  "clang-format.language.javascript.enable": false,
  "clang-format.language.java.enable": false,
  "clang-format.language.typescript.enable": false,
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "C_Cpp.codeAnalysis.clangTidy.enabled": true,
  "livePreview.autoRefreshPreview": "On Changes to Saved Files",
  "livePreview.portNumber": 5546,
  "workbench.statusBar.visible": false
}
